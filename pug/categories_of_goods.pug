extends core/base.pug

block title 
  title Категории товаров

block content

  .page 

    include core/header.pug

    .content 

      // main 
      .main 

        .main__img 
          img(src="img/main_bg.jpg", alt="")

        .container

          aside.main__menu 

            include blocks/subcatalog.pug

          main.main__content.main__content--padding-small

            // breadcrambs
            .breadcrambs
              ul 
                li 
                  a(href="#") Главная
                li 
                  a(href="#") Каталог продукции
            //	/breadcrambs

            // catalog-section
            .catalog-section
              h1.subtitle Стропы текстильные петлевые
              .row.gy-3 
                .col-md-4.col-sm-6 
                  a(href="#").catalog-section__item 
                    .catalog-section__img 
                      img(src="img/sibtal.jpg", alt="")
                    .catalog-section__title Стропы текстильные CТП
                .col-md-4.col-sm-6 
                  a(href="#").catalog-section__item 
                    .catalog-section__img 
                      img(src="img/catalog_20.jpg", alt="")
                    .catalog-section__title Стропы текстильные грузовые
                .col-md-4.col-sm-6 
                  a(href="#").catalog-section__item 
                    .catalog-section__img 
                      img(src="img/sibtal.jpg", alt="")
                    .catalog-section__title Стропы текстильные CТП
                .col-md-4.col-sm-6 
                  a(href="#").catalog-section__item 
                    .catalog-section__img 
                      img(src="img/sibtal.jpg", alt="")
                    .catalog-section__title Стропы текстильные CТП
                .col-md-4.col-sm-6 
