                    .catalog-section__img 
                      img(src="img/catalog_20.jpg", alt="")
                    .catalog-section__title Стропы текстильные грузовые
                .col-md-4.col-sm-6 
                  a(href="#").catalog-section__item 
                    .catalog-section__img 
                      img(src="img/sibtal.jpg", alt="")
                    .catalog-section__title Стропы текстильные CТП
            //	/catalog-section

